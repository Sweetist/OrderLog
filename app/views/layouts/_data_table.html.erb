<table class="table-bordered">
  <tr>
    <% @headers.each do |key, val| %>
      <th>
        <%= val.name %>
      </th> 
    <% end %>
  </tr>
  <% if @data.any? %>
    <% @data.each do |datum| %>
      <tr>
      <% datum.attributes.each do |key,val| %>
        <td><%= val %> </td>
      <% end %>
      <td><%= link_to "edit", datum, method: :get %></td>
      <td><%= link_to "delete", datum, method: :delete %></td>
      <td> </td>
      </tr>
    <% end %>
  <% end %>
</table>